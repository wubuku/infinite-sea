<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.suiinfinitesea.domain.rosterlocation">
  <class name="org.dddml.suiinfinitesea.domain.rosterlocation.AbstractRosterLocationState$SimpleRosterLocationState" table="roster_location">
    <id name="rosterObjectId" length="66">
      <column name="roster_object_id" sql-type="VARCHAR(66)" />
      <generator class="assigned" />
    </id>
    <version name="offChainVersion" column="off_chain_version" type="long" />
    <component name="coordinates" class="org.dddml.suiinfinitesea.domain.Coordinates">
      <property name="x">
        <column name="coordinates_x" />
      </property>
      <property name="y">
        <column name="coordinates_y" />
      </property>
    </component>
    <property name="status">
      <column name="status" />
    </property>
    <property name="version">
      <column name="version" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="active">
      <column name="active" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
